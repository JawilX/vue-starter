{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.15.9",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --open",
    "build": "vite build",
    "test": "vite build --mode development",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "up": "taze -I",
    "postinstall": "npx simple-git-hooks",
    "genapi:init": "gen-api init",
    "genapi": "gen-api now"
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.56.3",
    "@vueuse/core": "^12.4.0",
    "dayjs": "^1.11.13",
    "pinia": "^2.3.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.15.0",
    "@antfu/utils": "^8.1.0",
    "@arco-plugins/vite-vue": "^1.4.5",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@iconify-json/carbon": "^1.2.5",
    "@jawilx/gen-api": "^2.2.2",
    "@types/node": "^20.17.14",
    "@unocss/eslint-config": "^65.4.2",
    "@unocss/eslint-plugin": "^65.4.2",
    "@unocss/reset": "^65.4.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "eslint": "^9.18.0",
    "eslint-plugin-format": "^1.0.1",
    "less": "^4.2.2",
    "lint-staged": "^15.4.1",
    "pnpm": "^8.15.9",
    "simple-git-hooks": "^2.11.1",
    "taze": "^18.2.0",
    "typescript": "^5.7.3",
    "unocss": "^65.4.2",
    "unplugin-auto-import": "^19.0.0",
    "unplugin-vue-components": "^28.0.0",
    "unplugin-vue-router": "^0.11.1",
    "vite": "^6.0.11",
    "vue-tsc": "^2.2.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "pnpm commitlint --edit \"${1}\""
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
